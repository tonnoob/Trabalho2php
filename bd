CREATE DATABASE cadastroAluno;
create table aluno (
   RA int AUTO_INCREMENT ,
   nome varchar(60),
   nomePai varchar(60),
   nomeMae varchar(60),
   sexo varchar(9),
   CPF varchar(11),
   RG varchar(9),
   endereco varchar(60),
   numeroEndereco int (7),
   PRIMARY KEY (RA)
);

create table tbUsuario(
	idUsuario int PRIMARY key AUTO_INCREMENT,
    usuario varchar(40),
    senha varchar(40)
);

insert into tbUsuario values (null, 'admin','admin');
insert into tbUsuario values (null, 'maria','1234');